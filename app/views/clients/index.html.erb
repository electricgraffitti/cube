<% content_for :title do %>
  Clients
<% end %>
<% content_for :banner do  %>
  <div class="banner_content">
    <h2 class="banner_text header_white">
      <%= render :partial => "home/headers", :locals => {:objects => @headers} %>
    </h2>
  </div>
<% end %>

<div class="center">  
  <h2>Some Featured Clients</h2>
  <% @clients.each do |client| %>
    <%= render :partial => 'clients/client_post', :locals => {:client => client} %>
  <% end %>
</div>

<% content_for :sub_boxes do  %>
	<%= render :partial => 'layouts/sub_boxes' %>
<% end %>
